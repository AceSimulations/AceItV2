      document.getElementById('ext-rating').src = 'stars/' + rating.toString() + 'stars.png'
      document.getElementById('ext-person').textContent = name
      document.getElementById('ext-title').textContent = title
      document.getElementById('ext-message').textContent = message
    }

    function setIdOff(){
      console.log('function action')
      localStorage.setItem('personalPostId', "null")
      localStorage.setItem('comment', "null")
    }

    socket.on('server-update', () => {
      document.getElementById('update-ava').style.display = "block";
    })

    document.getElementById('comment-form').addEventListener('submit', (e) => {
      e.preventDefault()
      for(var i = 0; i < 20; i++) {
        var ranSeq = Math.floor(Math.random() * 62)
        var newNum = ranNum.slice(ranSeq, ranSeq + 1)
        id = id + newNum
      }
      localStorage.setItem('personalPostId', id)
      socket.emit('post', "@" + document.getElementById('name').value, document.getElementById('title').value, document.getElementById('message').value, starCount, id)
      document.getElementById('name').value = ""
      document.getElementById('title').value = ""
      document.getElementById('message').value = ""
      setStar('0')
      localStorage.setItem('comment', 'true')
      document.getElementById('comment-form-wrapper').style.display = "none"
      document.getElementById('comments-box').style.paddingBottom = "0px"
    })
